{
    "name": "dvelum/dvelum",
    "version": "3.0.0-dev",
    "type": "project",
    "description": "PHP/ExtJS-based web development platform",
    "keywords": [
        "dvelum",
        "php",
        "extjs",
        "IDE",
        "orm"
    ],
    "homepage": "https://github.com/dvelum/dvelum",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Kirill Yegorov",
            "email": "kirill@dvelum.com",
            "role": "Founder, Developer"
        },
        {
            "name": "Andrew Zamotaev",
            "role": "Developer"
        },
        {
            "name": "Sergey Leschenko",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "support@dvelum.com",
        "issues": "https://github.com/dvelum/dvelum/issues",
        "source": "https://github.com/dvelum/dvelum",
        "docs": "https://docs.dvelum.net"
    },
    "config": {
        "preferred-install": "dist",
        "classmap-authoritative": true
    },
    "require": {
        "php": ">=7.3.0",
        "ext-mysqli": "*",
        "ext-gd": "*",
        "ext-mbstring": "*",
        "ext-json": "*",
        "composer/installers": "~1.9",
        "oomphinc/composer-installers-extender": "^2.0",
        "psr/log": "^1.0",
        "dvelum/dvelum-core": "^3.1",
        "dvelum/extjs": "^6.2.0",
        "dvelum/module-designer": "^1.5",
        "matthiasmullie/minify": "~1.3",
        "ckeditor/ckeditor": "^4.9",
    },
    "extra": {
        "installer-types": [
            "js-library",
            "dvelum3-module",
            "dvelum3-extension",
            "project"
        ],
        "installer-paths": {
            "www/js/lib/ckeditor": [
                "ckeditor/ckeditor"
            ],
            "extensions/dvelum-core": [
                "dvelum/dvelum-core"
            ],
            "www/js/lib/{$name}": [
                "type:js-library"
            ],
            "modules/{$vendor}/{$name}": [
                "type:dvelum3-module"
            ],
            "extensions/{$vendor}/{$name}": [
                "type:dvelum3-extension"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phpstan/phpstan": "^0.12",
        "codacy/coverage": "dev-master",
        "composer/composer": "^2.0"
    },
    "repositories": {
        "chosen": {
            "type": "package",
            "package": {
                "name": "ckeditor/ckeditor",
                "version": "4.9.2",
                "type": "js-library",
                "dist": {
                    "url": "https://github.com/ckeditor/ckeditor4-releases/archive/full/4.9.2.zip",
                    "type": "zip"
                }
            }
        }
    }
}
