{
    "name": "dvelum/dvelum",
    "version": "3.0.0-dev",
    "type": "project",
    "description": "PHP/ExtJS-based web development platform",
    "keywords": [
        "dvelum",
        "php",
        "extjs",
        "IDE",
        "orm"
    ],
    "homepage": "https://github.com/dvelum/dvelum",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Kirill Yegorov",
            "role": "Founder, Developer"
        },
        {
            "name": "Andrew Zamotaev",
            "role": "Developer"
        },
        {
            "name": "Sergey Leschenko",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/dvelum/dvelum/issues",
        "source": "https://github.com/dvelum/dvelum"
    },
    "config": {
        "preferred-install": "dist",
        "classmap-authoritative": true
    },
    "require": {
        "php": ">=7.4.0",
        "ext-mysqli": "*",
        "ext-gd": "*",
        "ext-mbstring": "*",
        "ext-json": "*",
        "composer/installers": "~1.9",
        "oomphinc/composer-installers-extender": "^2.0",
        "psr/log": "^1.0",

        "dvelum/extjs": "^7.0.0",
        "dvelum/module-designer": "^1.6",

        "dvelum/cache": "^4.0",
        "dvelum/db": "^4.0",

        "matthiasmullie/minify": "~1.3",
        "laminas/laminas-db": "^2.11",
        "laminas/laminas-mail": "^2.12",
        "laminas/laminas-httphandlerrunner": "^1.3",
        "psr/log": "^1.1",
        "psr/container": "^2.0",
        "matthiasmullie/minify": "~1.3",
        "nyholm/psr7": "^1.4",
        "nyholm/psr7-server": "^1.0"
    },
    "extra": {
        "installer-types": [
            "js-library",
            "dvelum4-module",
            "dvelum4-extension",
            "project"
        ],
        "installer-paths": {
            "www/js/lib/ckeditor": [
                "ckeditor/ckeditor"
            ],
            "extensions/dvelum-core": [
                "dvelum/dvelum-core"
            ],
            "extensions/dvelum-orm": [
                "dvelum/dvelum-orm"
            ],
            "www/js/lib/{$name}": [
                "type:js-library"
            ],
            "modules/{$vendor}/{$name}": [
                "type:dvelum4-module"
            ],
            "extensions/{$vendor}/{$name}": [
                "type:dvelum4-extension"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phpstan/phpstan": "^0.12",
        "codacy/coverage": "dev-master",
        "composer/composer": "^2.0",
        "squizlabs/php_codesniffer": "^3.6",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "repositories": {
        "chosen": {
            "type": "package",
            "package": {
                "name": "ckeditor/ckeditor",
                "version": "4.9.2",
                "type": "js-library",
                "dist": {
                    "url": "https://github.com/ckeditor/ckeditor4-releases/archive/full/4.9.2.zip",
                    "type": "zip"
                }
            }
        }
    }
}
